{
  "name": "@tsers/rxjs",
  "version": "0.2.0",
  "private": false,
  "description": "TSERSful RxJS 5 bindings",
  "main": "dist/tsers.rxjs.js",
  "author": "Matti Lankinen <m.lankinen@iki.fi> (https://github.com/milankinen)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tsers-js/rxjs.git"
  },
  "keywords": [
    "rxjs",
    "reactive",
    "TSERS"
  ],
  "scripts": {
    "prepublish": "npm test && npm run dist",
    "test": "npm run lint && npm run mocha:all",
    "lint": "eslint src test",
    "mocha:all": "npm run mocha -- --recursive test",
    "mocha": "mocha --compilers js:babel-register -s 500",
    "dist": "npm run build && npm run minify",
    "build": "mkdir -p dist && rm -f dist/* && babel src/index.js > dist/tsers.rxjs.js",
    "minify": "uglifyjs -c -m -- dist/tsers.rxjs.js > dist/tsers.rxjs.min.js"
  },
  "dependencies": {
    "rxjs": "^5.0.0-beta.5"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-eslint": "^6.0.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.7.2",
    "eslint": "2.5.3",
    "mocha": "^2.4.5",
    "should": "^8.3.0",
    "uglify-js": "^2.6.2"
  }
}
